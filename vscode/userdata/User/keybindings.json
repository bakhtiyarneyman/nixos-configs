[
  { "key": "alt+down", "command": "-editor.action.moveLinesDownAction",
    "when": "editorTextFocus && !editorReadonly" },
  { "key": "alt+down", "command": "workbench.action.focusBelowGroup" },
  { "key": "alt+end", "command": "workbench.action.focusPanel",
    "when": "!panelFocus" },
  { "key": "alt+enter", "command": "editor.action.revealDefinition",
    "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor" },
  { "key": "alt+escape", "command": "workbench.action.focusActiveEditorGroup" },
  { "key": "alt+home", "command": "workbench.action.focusSideBar",
    "when": "!sideBarFocus" },
  { "key": "alt+left", "command": "workbench.action.focusLeftGroup" },
  { "key": "alt+pagedown", "command": "-scrollPageDown",
    "when": "textInputFocus" },
  { "key": "alt+pagedown", "command": "workbench.action.nextEditorInGroup",
    "when": "editorFocus" },
  { "key": "alt+pagedown", "command": "workbench.action.nextPanelView",
    "when": "panelFocus" },
  { "key": "alt+pageup", "command": "-scrollPageUp",
    "when": "textInputFocus" },
  { "key": "alt+pageup", "command": "workbench.action.previousEditorInGroup",
    "when": "editorFocus" },
  { "key": "alt+pageup", "command": "workbench.action.previousPanelView",
    "when": "panelFocus" },
  { "key": "alt+right", "command": "workbench.action.focusRightGroup" },
  { "key": "alt+space", "command": "workbench.action.toggleEditorWidths" },
  {
    "key": "alt+tab",
    "command": "workbench.action.compareEditor.focusOtherSide",
    "when": "textCompareEditorActive"
  },
  { "key": "alt+up", "command": "-editor.action.moveLinesUpAction",
    "when": "editorTextFocus && !editorReadonly" },
  { "key": "alt+up", "command": "workbench.action.focusAboveGroup" },
  { "key": "alt+z", "command": "-editor.action.toggleWordWrap" },
  { "key": "alt+z", "command": "workbench.action.navigateBack" },
  { "key": "ctrl+alt+d", "command": "addCursorsAtSearchResults",
    "when": "fileMatchOrMatchFocus && searchViewletVisible" },
  { "key": "ctrl+alt+d", "command": "editor.action.selectHighlights",
    "when": "editorFocus" },
  { "key": "ctrl+alt+d", "command": "selectAllSearchEditorMatches",
    "when": "inSearchEditor" },
  { "key": "ctrl+alt+l", "command": "editor.action.copyLinesUpAction",
    "when": "editorTextFocus && !editorReadonly" },
  { "key": "ctrl+alt+s", "command": "git.stageChange" },
  { "key": "ctrl+alt+s", "command": "git.stageSelectedRanges",
    "when": "isInDiffEditor" },
  { "key": "ctrl+down", "command": "-scrollLineDown",
    "when": "textInputFocus" },
  { "key": "ctrl+down", "command": "editor.action.moveLinesDownAction",
    "when": "editorTextFocus && !editorReadonly" },
  { "key": "ctrl+n", "command": "-workbench.action.files.newUntitledFile" },
  { "key": "ctrl+n", "command": "workbench.action.newWindow" },
  { "key": "ctrl+pagedown", "command": "-workbench.action.nextEditor" },
  { "key": "ctrl+pagedown", "command": "workbench.action.editor.nextChange",
    "when": "editorTextFocus" },
  { "key": "ctrl+pageup", "command": "-workbench.action.previousEditor" },
  { "key": "ctrl+pageup", "command": "workbench.action.editor.previousChange",
    "when": "editorTextFocus" },
  { "key": "ctrl+shift+down", "command": "scrollLineDown",
    "when": "textInputFocus" },
  { "key": "ctrl+shift+pagedown", "command": "-workbench.action.moveEditorRightInGroup" },
  { "key": "ctrl+shift+pagedown", "command": "scrollPageDown",
    "when": "textInputFocus" },
  { "key": "ctrl+shift+pageup", "command": "-workbench.action.moveEditorLeftInGroup" },
  { "key": "ctrl+shift+pageup", "command": "scrollPageUp",
    "when": "textInputFocus" },
  { "key": "ctrl+shift+up", "command": "scrollLineUp",
    "when": "textInputFocus" },
  { "key": "ctrl+shift+w", "command": "-workbench.action.closeWindow" },
  { "key": "ctrl+shift+w", "command": "workbench.action.reopenClosedEditor" },
  { "key": "ctrl+t", "command": "-workbench.action.showAllSymbols" },
  { "key": "ctrl+t", "command": "workbench.action.files.newUntitledFile" },
  { "key": "ctrl+up", "command": "-scrollLineUp",
    "when": "textInputFocus" },
  { "key": "ctrl+up", "command": "editor.action.moveLinesUpAction",
    "when": "editorTextFocus && !editorReadonly" },
  { "key": "ctrl+w", "command": "-workbench.action.closeGroup",
    "when": "activeEditorGroupEmpty && multipleEditorGroups" },
  { "key": "shift+alt+down", "command": "workbench.action.moveEditorToBelowGroup" },
  { "key": "shift+alt+end", "command": "workbench.action.togglePanel" },
  { "key": "shift+alt+home", "command": "workbench.action.toggleSidebarVisibility" },
  { "key": "shift+alt+left", "command": "workbench.action.moveEditorToLeftGroup" },
  { "key": "shift+alt+pagedown", "command": "workbench.action.moveEditorRightInGroup",
    "when": "editorFocus" },
  { "key": "shift+alt+pageup", "command": "workbench.action.moveEditorLeftInGroup",
    "when": "editorFocus" },
  { "key": "shift+alt+right", "command": "workbench.action.moveEditorToRightGroup" },
  { "key": "shift+alt+up", "command": "workbench.action.moveEditorToAboveGroup" },
  {
    "key": "shift+alt+z",
    "command": "workbench.action.navigateForward"
  },
  {
    "key": "ctrl+pagedown",
    "command": "workbench.action.compareEditor.nextChange",
    "when": "textCompareEditorVisible"
  },
  {
    "key": "ctrl+pageup",
    "command": "workbench.action.compareEditor.previousChange",
    "when": "textCompareEditorVisible"
  },
  {
    "key": "alt+tab",
    "command": "git.openChangeEditor",
    "when": "!textCompareEditorActive"
  },
  {
    "key": "alt+escape",
    "command": "git.openFile",
    "when": "textCompareEditorActive"
  },
  {
    "key": "ctrl+w",
    "command": "workbench.action.closeEditorsAndGroup",
    "when": "activeEditorGroupEmpty"
  }
]