{
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "One Dark Pro",
  "workbench.editor.enablePreview": false,
  "editor.tabSize": 2,
  "explorer.confirmDelete": false,
  "terminal.explorerKind": "external",
  "terminal.external.linuxExec": "termite",
  "terminal.integrated.fontFamily": "Inconsolata for Powerline",
  "sde.languageServerMode": "sourcekit-lsp",
  "swift.path.swift_driver_bin": "/swift-tensorflow-toolchain/usr/bin//swift",
  "sourcekit-lsp.serverPath": " /swift-tensorflow-toolchain/usr/bin/sourcekit-lsp",
  "editor.fontFamily": "'Fira Code'",
  "editor.fontLigatures": true,
  "editor.rulers": [100],
  "gitlens.advanced.telemetry.enabled": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "files.trimTrailingWhitespace": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "explorer.confirmDragAndDrop": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "files.autoSave": "off",
  "sourcekit-lsp.trace.server": "messages",
  "git.autofetch": true,
  "editor.wordWrap": "on",
  "git.confirmSync": false,
  "oneDarkPro.vivid": false,
  "window.menuBarVisibility": "toggle",
  "workbench.editor.closeEmptyGroups": false,
  "editor.suggestSelection": "first",
  "gradle.nestedProjects": true,
  "gradle.showStoppedDaemons": true,
  "window.zoomLevel": -1,
}